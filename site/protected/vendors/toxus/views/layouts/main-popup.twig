<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en" dir="ltr"> <![endif]-->
<!--[if IE 7]> <html class="no-js lt-ie9 lt-ie8" lang="en" dir="ltr"> <![endif]-->
<!--[if IE 8]> <html class="no-js lt-ie9" lang="en" dir="ltr"> <![endif]-->
<!--[if gt IE 8]><!--><html class="no-js" lang="en" dir="ltr"><!--<![endif]-->
<head>
	{{ this.addHeader() }}
	<meta charset="UTF-8" />		
<!--	<meta http-equiv="X-UA-Compatible" content="IE=5, IE=8, IE=9, IE=10" > -->
<title>{% block pageTitle %}{{ this.pageTitle }}{% endblock %}</title>
<!-- Meta Data ================ -->
<meta name="description" content="{{ App.params.description | default('site description') }}" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta name="apple-mobile-web-app-capable" content="yes" />
{{ this.registerCore('jquery') }}
{{ this.registerPackage('bootstrap') }}
{{ this.registerPackage('crisp') }}

<!--[if IE 7]>
<link rel="stylesheet" href="{{ App.baseUrl }}/css/font-awesome-ie7.min.css">
<![endif]-->
<link rel="stylesheet" href='http://fonts.googleapis.com/css?family=Ubuntu:400,700,700italic,400italic' />
<link rel="stylesheet" href='http://fonts.googleapis.com/css?family=Raleway:300,500,600,700' />
{% for css  in App.template.css %}
  {{ this.registerCssFile(css) }}
{% endfor %}

{# <script src="http://www.youtube.com/apiplayer?enablejsapi=1&version=3"></script> #}

</head>
<body class="index">
	<div id="page">
	{%block content%}The content {%endblock %}
	</div>
{% for script in App.template.scripts  %}
	{{ this.registerScriptFile(script, true) }}
{% endfor %}
</body>
</html>